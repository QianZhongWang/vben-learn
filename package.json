{
  "name": "vben-learn",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "commit": "echo 'czg'",
    "bootstrap": "yarn install",
    "serve": "yarn dev",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:test": "echo '打包📦测试文件'",
    "build:no-cache": " echo '将清空缓存后打包📦' && yarn clean:cache && yarn build",
    "report": "echo 'todo 报告生成中...' ",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "yarn build && vite preview",
    "previrw:dist": "vite preview",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cache:clean": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0 --ext \"{src/mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"src/**/*{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"src/**/*{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "test:unit": "jest",
    "test:gzip": "echo 'todo gzip 测试'",
    "test:br": "echo 'todo br 测试'",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && yarn bootstrap",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "gen:icon": "echo 'todo 图标生成'"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "autoprefix": "^1.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "global": "^4.4.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vue-tsc": "^0.39.5"
  }
}
